pipeline{
  agent any
    stages {
      stage('Build apps') {
        steps{
          sh('/scratch/wcpbt/software/maven/apache-maven-3.9.4/bin/mvn  -f java-tomcat-sample/pom.xml clean package')
        }
        post {
          success {
            echo "Archiving ...%..artifcats"
            archiveArtifacts artifacts: '**/*.war'
          }
        }
      }
   }
}
